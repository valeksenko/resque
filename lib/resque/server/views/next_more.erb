<%if  start - 20 >= 0 || start + 20 <= size%>
<p class='pagination'>
  <% if start - 20 >= 0 %>
    <a href="<%= current_page %>?start=<%= start - 20 %><%= params[:class] && "&class=#{ params[:class] }" %>" class='less'>&laquo; less</a>
  <% end %>
  <% if start + 20 <= size %>
    <a href="<%= current_page %>?start=<%= start + 20 %><%= params[:class] && "&class=#{ params[:class] }" %>" class='more'>more &raquo;</a>
  <% end %>
</p>
<%end%>